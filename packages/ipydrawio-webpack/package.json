        },
        "managers": [
          "pip",
          "conda"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/ipydrawio.git"
  },
  "scripts": {
    "build:pre": "python scripts/patch.py && python scripts/static.py"
  },
  "types": "lib/index.d.ts",
  "version": "14.2.6-alpha0"
}
